{
  "name": "@studioos/apps-api_nestjs",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/main.js",
    "lint": "eslint src test prisma --ext .ts",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" \"prisma/**/*.ts\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm run build && node --test dist/test/*.test.js dist/test/**/*.test.js",
    "openapi:generate": "pnpm exec tsx scripts/generate-openapi.ts",
    "openapi:publish": "pnpm run openapi:generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:status": "prisma migrate status --schema prisma/schema.prisma",
    "prisma:migrate:verify": "prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.991.0",
    "@aws-sdk/s3-request-presigner": "^3.991.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.16.3",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.58.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "yaml": "^2.8.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "^6.0.3",
    "prisma": "^6.16.3",
    "supertest": "^7.1.4",
    "tsx": "^4.20.5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
